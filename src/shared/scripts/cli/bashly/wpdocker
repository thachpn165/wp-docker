    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['domain']+x} ]]; then
    printf "missing required argument: DOMAIN\nusage: wpdocker wp reset password DOMAIN [NEW_PASSWORD]\n" >&2

    exit 1
  fi

}

